#include <stdio.h>

int main(){
    int X[1000],T,I=0,Y=0,M=0;
    scanf("%d",&T);
    while(I<1000){ //while para o programa fazer o loop até o numero indicado
        for(Y=0;Y<T;Y++){ // for para preencher o valor que foi digitado pelo usuario, Ex: se for 3 - 0,1,2 na sua posição do vetor.
         if(I==1000)   //condição para parar o programa assim que o valor for 1000, por que ja teria passado de 1000
           break;
        X[I]=Y; //x[i] vai preencher o vetor de 0 até 999, e y preenchera o valor digitado pelo usuario, conforme o ex a cima.
        I++;
        }
    } 
        for (M=0;M<1000;M++){ //for para printar as linhas
            printf("N[%d] = %d\n",M,X[M]); 
        } 
}